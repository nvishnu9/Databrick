   "source": [
    "SELECT\n",
    "  c.region,\n",
    "  ROUND(AVG(s.quantity * s.unit_price),2) AS avg_sale_value\n",
    "FROM sales s\n",
    "JOIN customers c\n",
    "  ON s.customer_id = c.customer_id\n",
    "GROUP BY c.region;"
   ]
  }
